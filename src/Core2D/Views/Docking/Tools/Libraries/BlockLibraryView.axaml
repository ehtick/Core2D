<UserControl x:Class="Core2D.Views.Docking.Tools.Libraries.BlockLibraryView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:libraries="using:Core2D.Views.Libraries"
             xmlns:renderer="using:Core2D.Views.Renderer"
             xmlns:vm="using:Core2D.ViewModels.Docking.Tools.Libraries"
             xmlns:vme="using:Core2D.ViewModels.Editor"
             xmlns:vmd="using:Core2D.ViewModels.Designer"
             mc:Ignorable="d"
             d:DesignWidth="800" d:DesignHeight="600"
             x:DataType="vm:BlockLibraryViewModel" x:CompileBindings="True">
  <Panel DataContext="{Binding Context}"
         x:DataType="vme:ProjectEditorViewModel"
         renderer:RendererOptions.Renderer="{Binding LibraryRenderer}"
         renderer:RendererOptions.Selection="{Binding Project}"
         renderer:RendererOptions.DataFlow="{Binding DataFlow}">
    <libraries:BlocksView DataContext="{Binding Project}"
                          d:DataContext="{x:Static vmd:DesignerContext.Project}" />
  </Panel>
</UserControl>
